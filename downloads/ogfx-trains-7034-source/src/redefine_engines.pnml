 * Redefine climate availability for  default engines
 * =============================================================================
 */

/* =====================================================
 * Steam / Diesel / Electric wagons.
 * ================================================== */

item(FEAT_TRAINS, kirbypaul, 00) {
    property {
        // We define this engine in a separate file
        climates_available:     ((USE_CLIMATE_TEMPERATE || USE_CLIMATE_ALL || USE_CLIMATE_ALL_BUT_TOYLAND) && param_provide_rail) ? bitmask(climate) : NO_CLIMATE;
        track_type:             NORMAL_RAIL;
    }
}

